{
  "name": "@farahub/framework",
  "version": "1.0.3",
  "description": "modular crm framework",
  "repository": "farzinkakh/farahub",
  "license": "MIT",
  "author": "farzinkakh <https://github.com/farzinkakh>",
  "exports": {
    "./facades": "./src/facades/index.js",
    "./facades/*": "./src/facades/*.js",
    "./foundation": "./src/foundation/index.js",
    "./foundation/*": "./src/foundation/*.js",
    ".": "./src/index.js"
  },
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon app/main.js --ignore apps/*/storage/* --ignore apps/*/ssl/* --ignore apps/*/public/*",
    "prod": "cross-env NODE_ENV=production pm2 start app/main.js --name farahub -i max --watch --ignore-watch='node_modules'",
    "seed": "cross-env NODE_ENV=development node database/seeders/main.js"
  },
  "dependencies": {
    "@hokify/agenda": "^6.3.0",
    "body-parser": "^1.20.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "express": "^4.18.1",
    "express-device": "^0.4.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.17.3",
    "express-subdomain": "^1.0.5",
    "express-validator": "^6.14.2",
    "i18n": "^0.15.1",
    "lodash": "^4.17.21",
    "middleware-async": "^1.3.5",
    "mime-types": "^2.1.35",
    "mongoose": "^6.3.1",
    "passport": "^0.6.0",
    "referrer-policy": "^1.2.0",
    "socket.io": "^4.5.1",
    "vhost": "^3.0.2",
    "xregexp": "^5.1.1"
  }
}